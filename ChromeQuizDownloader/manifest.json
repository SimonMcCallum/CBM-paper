{
  "manifest_version": 3,
  "name": "Canvas Quiz Downloader",
  "version": "1.0",
  "description": "Download current Canvas quiz page as QTI.",
  "action": {
    "default_title": "Download QTI"
  },
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*/"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
